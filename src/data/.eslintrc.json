{
    "env": {
        "browser": true
        , "node": true
    }
    , "extends": "eslint:recommended"
    , "rules": {
        "indent": [
            "error"
            , "tab"
        ]
        , "linebreak-style": [
            "error"
            , "unix"
        ]
        , "quotes": [
            "error"
            , "single"
        ]
        , "semi": [
            "error"
            , "always"
        ]
    }
    , "globals": {
        "footerCredits": true
        , "menuItems": true
    }
}